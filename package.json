    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "Prisma": "npm:prisma@^7.0.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.6.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
