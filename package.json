    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.2.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.1",
    "bcrypt": "^6.0.0",
